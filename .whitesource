{
  "checkRunSettings": {
    "displayMode": "diff",
    "useMendCheckNames": true,
    "vulnerableCheckRunConclusionLevel": "failure"
  },
  "issueSettings": {
    "assignees": [
      "EDM115"
    ],
    "customLabels": [
      "Mend: Security"
    ],
    "issueType": "DEPENDENCY",
    "minSeverityLevel": "LOW"
  },
  "python": {
    "installVirtualenv": true,
    "invokePipAsModule": true,
    "path": "python3.11"
  },
  "scanSettings": {
    "baseBranches": [
      "master"
    ],
    "configMode": "AUTO",
    "cloneSubmodules": true
  },
  "remediateSettings": {
    "enableRenovate": true,
    "workflowRules": {
      "enabled": true
    }
  }
}
